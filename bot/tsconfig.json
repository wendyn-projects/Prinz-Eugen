{
    "compilerOptions": {        
        "target": "es5",
        "module": "commonjs",
        "lib": ["es6"],
        "incremental": true,
        "resolveJsonModule": true,
        "outDir": "./build",
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,

        "noImplicitAny": true,
    },
    "include": [
        "src/**/*"
    ],
}